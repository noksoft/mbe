	<procedure id="infoMaqune" resultMap="maquneMap" parameterClass="classMaquneMap" >
		SELECT * FROM spr_Maqune(#idMaqune#,#idSubmarca#,#filterEstatusMaqune#)
	</procedure>	
	
	
